{
  "name": "FMP-frontend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "scripts": {
    "start": "npm run build && npm run server",
    "build": "webpack",
    "build:aot": "webpack --env.aot --env.client & webpack --env.aot --env.server",
    "build:prod": "webpack --env.aot --env.client -p & webpack --env.aot --env.server",
    "prebuild": "npm run clean",
    "prebuild:aot": "npm run clean",
    "prebuild:prod": "npm run clean",
    "clean": "rimraf dist",
    "server": "nodemon dist/server.js",
    "watch": "webpack --env.PORT='8000' --env.apiUrl='http://192.168.1.104:8443' --env.mode='dev'  --watch",
    "dev": "webpack-dev-server --history-api-fallback --content-base public --port 4200 --inline --open --env.apiUrl='http://192.168.1.104:8443' --env.mode='dev' --host 0.0.0.0",
    "start:dev": "webpack --env.PORT='8000' --env.apiUrl='http://192.168.1.104:8443' --env.mode='dev'",
    "restart:dev": "webpack --env.PORT='8000' --env.apiUrl='http://192.168.137.17:8081' && pm2 restart dist/server.js",
    "start:prod": "webpack --env.PORT='80' --env.apiUrl='http://52.36.29.171:8081' && pm2 start dist/server.js",
    "restart:prod": "webpack --env.PORT='80' --env.apiUrl='http://52.36.29.171:8081' && pm2 restart dist/server.js",
    "sprite-svg1": "del-cli -f src/assets/images/sprite.svg",
    "sprite-svg2": "svgo --config=svgo-sprite.json -f src/assets/images/sprites-svg/black-icons -o src/assets/images/sprites-svg/black-icons/tmp",
    "sprite-svg3": "svg-sprite-generate -d src/assets/images/sprites-svg/black-icons/tmp -o src/assets/images/sprite.svg",
    "sprite-svg4": "del-cli -f src/assets/images/sprites-svg/black-icons/tmp",
    "make-sprite-black-svg": "npm run sprite-svg1 && npm run sprite-svg2 && npm run sprite-svg3 && npm run sprite-svg4",
    "optimize-svg1": "del-cli -f src/assets/images/sprite2.svg",
    "optimize-svg2": "svgo --config=svgo-optimize.json -f src/assets/images/sprites-svg/color-icons -o src/assets/images/sprites-svg/color-icons/tmp",
    "optimize-svg3": "svg-sprite-generate -d src/assets/images/sprites-svg/color-icons/tmp -o src/assets/images/sprite2.svg",
    "optimize-svg4": "del-cli -f src/assets/images/sprites-svg/color-icons/tmp",
    "make-sprite2-color-svg": "npm run optimize-svg1 && npm run optimize-svg2 && npm run optimize-svg3 && npm run optimize-svg4"
  },
  "engines": {
    "node": ">=7.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "4.3.0",
    "@angular/cli": "^1.4.9",
    "@angular/common": "4.3.0",
    "@angular/compiler": "4.3.0",
    "@angular/core": "4.3.0",
    "@angular/forms": "4.3.0",
    "@angular/http": "4.3.0",
    "@angular/platform-browser": "4.3.0",
    "@angular/platform-browser-dynamic": "4.3.0",
    "@angular/platform-server": "4.3.0",
    "@angular/router": "4.3.0",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.28",
    "@types/stripe": "2.0.2",
    "angular2-file-drop": "0.2.0",
    "angular2-tag-input": "1.2.3",
    "angular2-text-mask": "8.0.2",
    "angulartics2": "2.0.0",
    "body-parser": "1.17.2",
    "bootstrap": "4.0.0-alpha.6",
    "compression-webpack-plugin": "1.0.0",
    "express": "4.15.4",
    "file-saver": "1.3.3",
    "ng2-google-place-autocomplete": "1.4.2",
    "ng2-truncate": "1.3.7",
    "ngx-facebook": "2.4.0",
    "ngx-stripe": "^0.2.3",
    "primeng": "4.1.2",
    "request": "2.81.0",
    "rxjs": "5.4.2",
    "xhr2": "0.1.4",
    "zone.js": "0.8.13"
  },
  "devDependencies": {
    "@angular/compiler-cli": "4.3.0",
    "@ngtools/webpack": "1.2.14",
    "@types/express": "4.0.35",
    "@types/node": "7.0.8",
    "angular2-router-loader": "0.3.5",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.1.2",
    "clean-webpack-plugin": "0.1.16",
    "cors": "2.8.3",
    "css-loader": "0.27.3",
    "del-cli": "0.2.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.10.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "http-proxy": "1.16.2",
    "node-sass": "4.5.1",
    "nodemon": "1.11.0",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "2.0.2",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.3",
    "script-ext-html-webpack-plugin": "1.7.1",
    "style-loader": "0.16.0",
    "svg-sprite-generator": "0.0.7",
    "svgo": "0.7.2",
    "ts-helpers": "1.1.2",
    "tslint": "~4.5.0",
    "typescript": "2.2.1",
    "typings": "2.1.0",
    "uglifyjs-webpack-plugin": "0.3.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "1.16.2",
    "webpack-merge": "4.0.0"
  }
}
